      window.sr = ScrollReveal();

    sr.reveal('.card', {
      duration: 2000,
      origin: 'bottom',
distance: '300px'
    });

    sr.reveal('.lrs', {
      duration: 2000,
      origin: 'left',
    
    });
     sr.reveal('.enca', {
      duration: 2000,
      origin: 'right',
      distance: '400px'
    });
      sr.reveal('.enca2', {
      duration: 6000,
      origin: 'bottom',
      distance: '400px'
    });

    sr.reveal('.enca3', {
      duration: 4000,
      origin: 'right',
      distance: '500px'
    });

    sr.reveal('.menu', {
      duration: 2000,
      delay: 1000, 
      origin: 'bottom'
    });

    sr.reveal('.scrou-l', {
      duration: 2000,
      origin: 'left',
      distance: '300px'
    
    });

    document.querySelectorAll('a[href^="#"]').forEach(anchor => {
      anchor.addEventListener('click', function (e) {
        e.preventDefault();

        document.querySelector(this.getAttribute('href')).scrollIntoView({
          behavior: 'smooth'
        });
      });
    });
    </script>

